{
    "openapi": "3.0.0",
    "info": {
        "version": "1.0.23",
        "title": "api-cauculate-property-value",
        "description": "API que retorna os valor total do imóvel pela quantidade do metro quadrado de acordo com o CEP.",
        "license": {
            "name": "MIT",
            "url": "https://opensource.org/licenses/MIT"
        }
    },
    "paths": {
        "/CalculatePropertyValue": {
            "get": {
                "tags": [
                    "Valor de um imóvel"
                ],
                "summary": "Retorna o valor do imóvel através do CEP e quantidade de metros quadrados.",
                "responses": {
                    "200": {
                        "description": "OK"
                    }
                }
            },
            "parameters": [
                {
                    "name": "cep",
                    "in": "query",
                    "required": true,
                    "description": "Referente ao local do imóvel",
                    "type": "string"
                },
                {
                    "name": "squareMeters",
                    "in": "query",
                    "required": true,
                    "description": "Referente a quantidade de metros quadrados",
                    "type": "string"
                }
            ],
            "responses": {
                "200": {
                    "description": "OK",
                    "schema": {
                        "$ref": "#/definitions/ValueProperty"
                    }
                }
            }
        }
    },
    "definitions": {
        "ValueProperty": {
            "type": "object",
            "properties": {
                "Cep": {
                    "type": "string"
                },
                "PropertyValue": {
                    "type": "decimal"
                }
            }
        }
    }
}
